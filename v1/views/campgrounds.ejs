<% include partials/header %>




<div class="container">
<div class="container">
<header class="jumbotron">
    <h1>Welcome to CampGrounds</h1>
    <p>View our handpicked Campgrounds</p>
<a class="btn btn-primary btn-large" href="/campgrounds/new">Add New Campground</a>
</header>
</div>
<div class="row text-center" style="display: flex; flex-wrap: wrap;">
    <%campgrounds.forEach(function(camp){%>
    <div class="col-md-3 col-sm-6">
        <div class="thumbnail">
                <img class="img-thumbnail" src="<%=camp.image%>">
                <div class="caption"><h4><%=camp.name%></h4></div>
        </div>
    </div>      


<%})%>
</div>
</div>
<% include partials/footer.ejs %>